<section class="container">
    <div class="column">

      {{#each posts}}
      <div class="row">
        <img src="{{image}}" alt="Placeholder image">
        <div class="content" {{!--contenteditable="true"--}}>
          <a href="/articles/{{_id}}">
            <h2>{{title}}</h2>
          </a>
          <p>{{{content}}}</p>
          <div class="date">
          <p>Publié par {{auteur}}, le {{moment creatDate format="DD-MM-YY"}}</p>
          </div>  

        </div>
      </div>
    </div>
    {{/each}}


  </div>
</section>

