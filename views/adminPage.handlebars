<section>
  <!--for demo wrap-->
  <h1>Tableau des utilisateurs</h1>
  <div class="tbl-header">
    <table cellpadding="0" cellspacing="0" border="0">
      <thead>
        <tr>
          <th>Ordre</th>
          <th>Pseudo</th>
          <th>Nom</th>
          <th>Email</th>
          <th>Id</th>
          
        </tr>
      </thead>
    </table>
  </div>
  <div class="tbl-content">
    <table cellpadding="0" cellspacing="0" border="0">
      <tbody>
      {{#each usr }}
        <tr>
          <td name="lsNumber">{{lsNumber}}</td>
          <td name="pseudo">{{pseudo}}</td>
          <td name="name">{{name}}</td>
          <td name="email">{{email}}</td>
          <td name="id">{{id}}</td>      
        </tr>
      {{/each}}
      </tbody>
    </table>
  </div>
</section>
<a><input class="btn" type=button onclick=window.location.href='/'; value=retour />
